package com.behavioral.mediator.collleague;

import com.behavioral.mediator.ChatRoom;

public class User {
	private ChatRoom mediator;
	
	private String id;
	private String name;
	
	public User(ChatRoom mediator, String id, String name) {
		super();
		this.mediator = mediator;
		this.id = id;
		this.name = name;
	}
	
	
}
